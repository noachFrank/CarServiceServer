{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ConStr": "YOUR_PRODUCTION_CONNECTION_STRING"
  },
  "JwtSettings": {
    "SecretKey": "YOUR_PRODUCTION_JWT_SECRET_KEY_AT_LEAST_32_CHARACTERS",
    "Issuer": "DispatchApp",
    "Audience": "DispatchAppUsers",
    "ExpirationMinutes": "60"
  },
  "EmailSettings": {
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": "587",
    "SmtpUsername": "YOUR_EMAIL@gmail.com",
    "SmtpPassword": "YOUR_APP_PASSWORD",
    "FromEmail": "YOUR_EMAIL@gmail.com",
    "FromName": "Dispatch App"
  },
  "GoogleMaps": {
    "ApiKey": "YOUR_GOOGLE_MAPS_API_KEY"
  },
  "DriverAvailability": {
    "DefaultTravelTimeMinutes": 20,
    "BaseGracePeriodMinutes": 30,
    "LongCallThresholdMinutes": 45,
    "GracePeriodScalingEnabled": true
  },
  "SquareSettings": {
    "ApplicationId": "YOUR_SQUARE_PRODUCTION_APP_ID",
    "AccessToken": "YOUR_SQUARE_PRODUCTION_ACCESS_TOKEN",
    "Environment": "Production",
    "LocationId": "YOUR_SQUARE_LOCATION_ID"
  },
  "AllowedOrigins": [
    "https://your-production-domain.com"
  ]
}
